{
  "name": "react-wood-duck",
  "version": "0.1.23",
  "description": "The CWDS component library (react-wood-duck) is for all CWDS digital service teams to contribute to and use in order to provide a consistent look, feel, and behavior for the overall CWDS application.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ca-cwds/react-wood-duck.git"
  },
  "author": "Naga Pradeep Gajula",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "keywords": [
    "react-component",
    "react"
  ],
  "scripts": {
    "prepublish": "babel --plugins transform-es2015-modules-umd src --out-dir ./dist && node-sass src/styles/application.scss dist/styles/application.scss && node-sass src/styles/application.scss dist/styles/application.css && copyfiles -f ./src/images/* ./dist/images && copyfiles -f ./src/styles/icon/* ./dist/styles/icon && copyfiles -f ./src/fonts/* ./dist/fonts && echo 'all dist copied.'",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "build:dev": "webpack",
    "buid:prod": "NODE_ENV=production webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.7",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^1.1.0",
    "jasmine": "^2.7.0",
    "jasmine-core": "^2.7.0",
    "jsdom": "^8.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "path": "^0.12.7",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^1.17.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bootstrap": "^3.3.7",
    "copyfiles": "^1.2.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "react-datetime": "^2.9.0",
    "react-select": "^1.0.0-rc.5"
  }
